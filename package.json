{
  "name": "ember-service-worker",
  "version": "0.7.0",
  "description": "A pluggable approach to Service Workers for Ember.js",
  "scripts": {
    "pretest": "./scripts/pretest.sh",
    "lint:hbs": "ember-template-lint .",
    "test": "mocha node-tests",
    "test:builder": "mocha node-tests/service-worker-builder-test.js",
    "test:acceptance": "mocha node-tests/acceptance-test.js",
    "test:ember": "ember test",
    "test:all": "ember try:each",
    "fastboottest": "ember fastboot:test"
  },
  "homepage": "http://ember-service-worker.com",
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "broccoli-test-helpers": "0.0.9",
    "broccoli-asset-rev": "^3.0.0",
    "ember-ajax": "^3.1.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-resolver": "^5.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-dependency-checker": "^3.0.0",
    "chai": "^4.2.0",
    "ember-cli": "^3.4.3",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-fastboot-addon-tests": "^0.5.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-source": "^3.4.5",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "loader.js": "^4.7.0",
    "mocha": "^5.2.0",
    "qunit-dom": "^0.8.0",
    "rimraf": "^2.5.3"
  },
  "keywords": [
    "ember-addon",
    "ember-service-worker",
    "service worker"
